<h4>Users</h4>
<div class="table-responsive">
  <table class="table">
    <thead class="thead">
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Mobile</th>
        <th>Gender</th>
        <th>Edit</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user|%>
      <tr>
        <td><%= user.first_name%></td>
        <td><%= user.last_name%></td>
        <td><%= user.email%></td>
        <td><%= user.mobile%></td>
        <td><%= user.gender%></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>