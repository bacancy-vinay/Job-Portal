<%= form_with(model:[@user, @resume]) do |f| %>
<div class="field">
  <%= f.hidden_field :user_id, value: @user.id %>
</div>

<div class="form-group">
  <%= f.label :file_name %>
  <%= f.text_field :file_name, class: "form-control" %>
</div>
<div class="form-group">
  <%= f.label :resume_file %>
  <%= f.file_field :resume_file, class: "form-control" %>
</div>

<div class="form-group">
  <%= f.submit :Submit, class: "btn btn-primary" %>
</div>
<% end %>