<%= form_with(model: [@company,@review]) do |f| %>

  <div class="form-group">
    <%= f.label :Rating %>
    <%= f.text_field :rating, class: "form-control" %>
  </div>
  
  <div class="form-group">
    <%= f.label :Review %>
    <%= f.text_area :review_desc, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.submit :Submit, class: "btn btn-primary" %>
  </div>
<% end %>